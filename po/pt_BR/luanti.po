msgstr "Velocidade ao esgueirar-se, em nós (blocos) por segundo."

#: src/settings_translation_file.cpp
msgid "Soft clouds"
msgstr "Nuvens suaves"

#: src/settings_translation_file.cpp
msgid "Soft shadow radius"
msgstr "Raio das sombras suaves"

